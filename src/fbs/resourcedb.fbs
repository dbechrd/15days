namespace DB;

table Animation {
    name:string;
    frame_start:int;
    frame_count:int;
}

table Spritesheet {
    name:string;
    texture_path:string;
    cell_count:int;
    cell_width:int;
    cell_height:int;
    animations:[Animation];
}

table ResourceDB {
    name:string;
    spritesheets:[Spritesheet];
}

root_type ResourceDB;

file_identifier "15dy";
file_extension "fbb";